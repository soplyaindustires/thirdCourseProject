{
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "printWidth": 120,
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "singleAttributePerLine": true,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "ignore"
}